<%= form_for(@job) do |f| %>
  <%= f.label :title %><br>
  <%= f.text_field :title %><br>
  <%= f.label :company_name %><br>
  <%= f.collection_select :company_id, Company.order(:name),:id,:name, include_blank: false %><br>
  <%= f.label :city %><br>
  <%= f.text_field :city %><br>
  <%= f.label :state %><br>
  <%= f.text_field :state %><br>
  <%= f.label :description %><br>
  <%= f.text_field :description %><br>
  <%= f.label :salary %><br>
  <%= f.number_field :salary %><br>
  <%= f.submit %>
<% end %>